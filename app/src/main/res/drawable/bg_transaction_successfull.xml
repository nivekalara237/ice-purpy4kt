<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="0dp"
                android:color="#111"/>
            <gradient
                android:type="linear"
                android:angle="-90"
                android:startColor="#04a378"
                android:centerColor="#05a356"
                android:endColor="#08ce6e"/>
            <corners
                android:radius="7dp"/>
        </shape>
    </item>
</selector>

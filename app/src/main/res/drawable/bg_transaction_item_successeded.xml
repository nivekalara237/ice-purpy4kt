<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="20dp"/>
            <gradient
                android:endColor="#D6EDFF"
                android:centerColor="#D6EDFF"
                android:startColor="#D6EDFF"/>
            <solid
                android:color="#D6EDFF"/>
            <stroke
                android:width="0dp"
                android:color="#8351f1"/>
        </shape>
    </item>
    <item
        android:right="5dp"
        android:left="5dp">
        <bitmap
            android:src="@drawable/ic_check_white_24"
            android:gravity="center_vertical|right"
            android:tint="#688EEF"/>
    </item>
</layer-list>